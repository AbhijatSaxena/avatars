{
  "name": "@dicebear/avatars",
  "version": "0.0.4",
  "description": "JavaScript Avatar Generator",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "dependencies": {
    "@types/async": "^2.0.40",
    "@types/chance": "^0.7.32",
    "@types/node": "^7.0.12",
    "@types/object-assign": "^4.0.30",
    "async": "^2.3.0",
    "canvas": "^1.6.5",
    "chance": "^1.0.6",
    "object-assign": "^4.1.1",
    "onecolor": "^3.0.4"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.14",
    "typescript": "^2.2.1"
  },
  "scripts": {
    "prebuild": "rm -r dist/* lib/* || true",
    "build": "./node_modules/.bin/tsc; ./node_modules/.bin/browserify lib/index.js -s Avatars > dist/avatars.js"
  },
  "author": "Florian KÃ¶rner <contact@dicebear.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiceBear/avatars.git"
  },
  "keywords": [
    "avatar",
    "generator",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/DiceBear/avatars/issues"
  },
  "homepage": "https://github.com/DiceBear/avatars#readme",
  "browser": {
    "./lib/index.js": "./lib/index.browser.js",
    "./lib/helper/canvas.js": "./lib/helper/canvas.browser.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "brfs"
    ]
  },
  "browserify-shim": {
    "chance": "global:window",
    "async": "global:async",
    "onecolor": "global:one.color"
  }
